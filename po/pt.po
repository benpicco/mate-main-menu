msgid "possible values = 0 [Applications], 1 [Documents], 2 [Places]"
msgstr "valores possíveis = 0 [Aplicações], 1 [Documentos], 2 [Locais]"

#: ../main-menu/etc/slab.schemas.in.in.h:33
msgid ""
